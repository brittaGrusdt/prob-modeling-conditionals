var state_prior = cache(function(bias) {
  var distr = Infer({method:'enumerate', model:function(){
      var bn = uniformDraw(globalStore.bns)
      var Table = bn.Table
      var cn = bn.cn
      var state = {"table": Table,
                   "cn": cn,
                   "id": bn["bn_id"]};
      let logl = bn.ll
      let log_val = !logl ? -Infinity : logl;
      factor(log_val);
      return {"bn": state}
    }});
    return(distr)
})
